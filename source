local Button = MainTab:CreateButton({
   Name = "United Power",
   Callback = function()
            -- Only works for users running your script
if not isfile or not game or not game:IsLoaded() then return end

local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local debris = game:GetService("Debris")
local RunService = game:GetService("RunService")

-- Helper: Clean animations
local function stopAnims()
    for _, t in ipairs(char:GetDescendants()) do
        if t:IsA("Animation") or t:IsA("AnimationTrack") then
            t:Stop()
        end
    end
end

-- Custom float animation
local function agathaFloat()
    local float = Instance.new("BodyPosition")
    float.Position = hrp.Position + Vector3.new(0, 5, 0)
    float.MaxForce = Vector3.new(0, math.huge, 0)
    float.Damping = 0
    float.P = 5000
    float.Parent = hrp

    -- Agatha arm & pose setup
    local hum = char:FindFirstChildOfClass("Humanoid")
    if hum then
        hum:ChangeState(Enum.HumanoidStateType.Physics)
    end

    for _, limb in ipairs(char:GetDescendants()) do
        if limb:IsA("Motor6D") and (limb.Name:lower():find("shoulder") or limb.Name:lower():find("hip")) then
            limb:Destroy()
        end
    end

    debris:AddItem(float, 5)
end

-- Witchy beam attack effect
local function cosmicBlast()
    -- Purple orb
    local sphere = Instance.new("Part")
    sphere.Shape = Enum.PartType.Ball
    sphere.Size = Vector3.new(8,8,8)
    sphere.Anchored = true
    sphere.CanCollide = false
    sphere.Material = Enum.Material.ForceField
    sphere.Color = Color3.fromRGB(145, 85, 255)
    sphere.Transparency = 0.2
    sphere.Position = hrp.Position
    sphere.Parent = workspace

    -- Energy beam
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= lp and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local targetHRP = plr.Character.HumanoidRootPart
            if (targetHRP.Position - hrp.Position).Magnitude <= 50 then
                local beamPart = Instance.new("Part")
                beamPart.Anchored = true
   end,
})

