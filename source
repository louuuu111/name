game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("NonFightEvents"):WaitForChild("StaffPanel"):FireServer(unpack(args))
   end,
})

local Button = United PowerTab:CreateButton({
   Name = "Summoning Aura",
   Callback = function()
            local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Debris = game:GetService("Debris")

local LocalPlayer = Players.LocalPlayer

-- Whitelist of allowed players
local whitelist = {
    "BaddiesCarbbiean", -- Replace with actual player usernames
    "therealbadddollyy",
    
}

-- Function to check if a player is whitelisted
local function isWhitelisted(playerName)
    for _, name in ipairs(whitelist) do
        if name == playerName then
            return true
        end
    end
    return false
end

-- Function to create the aura effect
local function createAura(Character)
    local HRP = Character:WaitForChild("HumanoidRootPart")

    -- Floor Ring
    local Ring = Instance.new("Part")
    Ring.Anchored = true
    Ring.CanCollide = false
    Ring.Size = Vector3.new(1, 0.2, 1)
    Ring.Position = HRP.Position - Vector3.new(0, 2.6, 0)
    Ring.Material = Enum.Material.Neon
    Ring.Color = Color3.fromRGB(200, 0, 255)
    Ring.Transparency = 0.25
    Ring.Shape = Enum.PartType.Cylinder
    Ring.Name = "CovenRing"
    Ring.Parent = Character
    Ring.Orientation = Vector3.new(0, 0, 90)

    local rot = Instance.new("BodyAngularVelocity")
    rot.AngularVelocity = Vector3.new(0, 3, 0)
    rot.MaxTorque = Vector3.new(0, math.huge, 0)
    rot.Parent = Ring
    Debris:AddItem(Ring, 10)

    -- Sparkles
    local Sparkle = Instance.new("ParticleEmitter")
    Sparkle.Texture = "rbxassetid://258128463" -- Soft sparkle texture
    Sparkle.Rate = 25
    Sparkle.Lifetime = NumberRange.new(1.5)
    Sparkle.Speed = NumberRange.new(0.5, 1)
    Sparkle.Size = NumberSequence.new({NumberSequenceKeypoint.new(0, 0.4), NumberSequenceKeypoint.new(1, 0)})
    Sparkle.Color = ColorSequence.new(Color3.fromRGB(255, 200, 255))
    Sparkle.Parent = HRP
    Debris:AddItem(Sparkle, 10)

    -- Misty Orbs
    local Mist = Instance.new("ParticleEmitter")
    Mist.Texture = "rbxassetid://98330050"
    Mist.Rate = 10
    Mist.Lifetime = NumberRange.new(2)
    Mist.Speed = NumberRange.new(0.2, 0.5)
    Mist.Size = NumberSequence.new(1)
    Mist.Transparency = NumberSequence.new(0.3)
    Mist.Color = ColorSequence.new(Color3.fromRGB(180, 120, 255))
    Mist.Parent = HRP
    Debris:AddItem(Mist, 10)

    -- Lightning Flicks
    for i = 1, 3 do
        local Flick = Instance.new("Part")
        Flick.Anchored = true
        Flick.CanCollide = false
        Flick.Size = Vector3.new(0.2, 3, 0.2)
        Flick.Material = Enum.Material.Neon
        Flick.Color = Color3.fromRGB(200, 0, 255)
        Flick.Transparency = 0.3
        Flick.CFrame = HRP.CFrame * CFrame.new(math.random(-3, 3), math.random(1, 3), math.random(-3, 3))
        Flick.Parent = workspace
        Debris:AddItem(Flick, 0.3)
    end

    -- Witchy Whisper Sound
    local Sound = Instance.new("Sound")
    Sound.SoundId = "rbxassetid://9127154036" -- whisper loop vibe
    Sound.Looped = false
    Sound.Volume = 1
    Sound.PlayOnRemove = true
    Sound.Parent = HRP
    Sound:Destroy()
end

-- Chat detection with whitelist check
local function monitorChat()
    local ChatEvent = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
    ChatEvent.OnClientEvent:Connect(function(data)
        if data.FromSpeaker == LocalPlayer.Name then
            local msg = string.lower(data.Message)
            if msg == "we rise" then
                -- Check if player is whitelisted
                if isWhitelisted(LocalPlayer.Name) then
                    local Char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                    createAura(Char)
                end
            end
        end
    end)
end

monitorChat()
   end,
})
